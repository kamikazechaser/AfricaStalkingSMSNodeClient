<div class="row">
	<div class="col-lg-12">
		<div class="box">
			<div class="box-header">
				<h2 class="box-title">{{name}}</h2>
				{{>export}}
			</div>
			<!-- /.box-header -->
			<div class="box-body">
				<div class="col-lg-12">
					<table class="table datatable table-bordered table-hover">
						<thead>
							<tr>
								<th>Group name</th>
								<th></th>
							</tr>
						</thead>
						<tbody>
							{{#each groups as |group|}}
							<tr>
								<td>{{group.name}}</td>
								<td>
									<div class="btn-group">
										<a href="{{view_link}}" class="btn btn-xs btn-info">View</a>
										<a href="{{delete_link}}" class="btn btn-xs btn-danger">delete</a>
									</div>
								</td>
							</tr>
							{{/each}}


						</tbody>

					</table>
				</div>
			</div>

		</div>

	</div>
</div>




