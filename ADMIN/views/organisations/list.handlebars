<div class="row">
	<div class="col-lg-12">
		<div class="box">
			<div class="box-header">
				<h2 class="box-title">{{name}}</h2>
				{{>export}}
			</div>
			<!-- /.box-header -->
			<div class="box-body">
				<div class="col-lg-12">
					<table class="table datatable table-bordered table-hover">
						<thead>
							<tr>
								<th>Name</th>
								<th>Location</th>
								<th>Details</th>
								<th></th>
							</tr>
						</thead>
						<tbody>
							{{#each organisations as |org|}}
							<tr>
								<td>{{org.name}}</td>
								<td>{{org.locations}}</td>
								<td>{{org.details}}</td>
								<td>
									<div class="btn-group">
										<a href="{{view_link}}" class="btn btn-xs btn-info">View info</a>
									</div>
								</td>
							</tr>
							{{/each}}
						</tbody>
					</table>
				</div>
			</div>

		</div>

	</div>
</div>




