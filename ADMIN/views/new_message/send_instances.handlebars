<div class="row">
	<div class="col-lg-12">
		<div class="box">
			<div class="box-header">
				<h2 class="box-title">{{name}}</h2>
				{{>export}}
			</div>
			<!-- /.box-header -->
			<div class="box-body">
				<div class="col-lg-12">
					<table class="table datatable table-bordered table-hover">
						<thead>
							<tr>
								<th>Time</th>
								<th>Sender</th>
								<th>Contacts Reached</th>
								<th>Cost</th>
								<th></th>
							</tr>
						</thead>
						<tbody>
							{{#each instances as |instance|}}
							<tr>
								<td>{{instance.time}}</td>
								<td>{{instance.admin}}</td>
								<td>{{instance.contactsReached}}</td>
								<td>{{instance.cost}}</td>
								<td>
									<div class="btn-group">
										<a href="{{instance.view_link}}" class="btn btn-xs btn-info">View More</a>
									</div>
								</td>
							</tr>
							{{/each}}
						</tbody>
					</table>
				</div>
			</div>

		</div>

	</div>
</div>




