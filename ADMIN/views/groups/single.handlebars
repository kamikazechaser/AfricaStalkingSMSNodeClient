<div class="row">
	<div class="col-lg-12">
		<div class="box">
			<div class="box-header">
				<h2 class="box-title">{{name}}</h2>
				{{>export}}
			</div>
			<!-- /.box-header -->
			<div class="box-body">
				<div class="col-lg-12">
					<table class="table datatable table-bordered table-hover">
						<thead>
							<tr>
								<th>Contact name</th>
							</tr>
						</thead>
						<tbody>
							{{#each contacts as |contact|}}
							<tr>
								<td>{{contact.contact_name}}</td>
							</tr>
							{{/each}}
						</tbody>

					</table>
				</div>
			</div>

		</div>

	</div>
</div>




