<table class="table datatable table-bordered table-hover">
						<thead>
							<tr>
								<th>Date</th>
								<th>Ammount</th>
								<th>Channel</th>
								<th>Contact</th>
								<th>Reference Id</th>
							</tr>
						</thead>
						<tbody>
							{{#each payments as |payment|}}
							<tr>
								<td>{{payment.date}}</td>
								<td>{{payment.ammount}}</td>
								<td>{{payment.channel}}</td>
								<td>{{payment.contact}}</td>
								<td>{{payment.ref_id}}</td>
							</tr>
							{{/each}}


						</tbody>

					</table>