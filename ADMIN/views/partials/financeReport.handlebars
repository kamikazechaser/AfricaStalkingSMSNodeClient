<div class="row">
  <div class="col-lg-12">
    <div class="box">
      <div class="box-header">
        <h2 class="box-title">{{branch_session.name}}</h2>
        {{>export}}
      </div>
      <!-- /.box-header -->
      <div class="box-body">
        <div class="divider"></div>
        <div class="col-lg-12">
          <h4 class="box-title">Invoices</h4>
         <table class="table datatable table-bordered table-hover">
            <thead>
              <tr>
                <th>Date</th>
                <th>ammount</th>
                <th>session</th>
              </tr>
            </thead>
            <tbody>
              {{#each financials.invoices as |payment|}}
              <tr>
                <td>{{payment.date}}</td>
                <td>{{payment.totalPrice}}</td>
                <td>{{payment.session_name}}</td>
              </tr>
              {{/each}}


            </tbody>

          </table>
        </div>
      </div>

    </div>

  </div>
</div>

